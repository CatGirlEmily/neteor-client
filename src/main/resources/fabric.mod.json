{
  "schemaVersion": 1,
  "name": "Neteor Client",
  "id": "neteor-client",
  "version": "${version}",
  "description": "meteor but cheatless",
  "authors": [
    "capsoffpls",
    "meteordevelopment"
  ],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/neteor-client/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "neteordevelopment.neteorclient.NeteorClient"
    ],
    "modmenu": [
      "neteordevelopment.neteorclient.ModMenuIntegration"
    ]
  },
  "mixins": [
    "neteor-client.mixins.json",
    "neteor-client-sodium.mixins.json",
    "neteor-client-lithium.mixins.json",
    "neteor-client-viafabricplus.mixins.json"
  ],
  "accessWidener": "neteor-client.accesswidener",
  "custom": {
    "neteor-client:build_number": "${build_number}",
    "neteor-client:color": "145,61,226",
    "neteor-client:commit": "${commit}"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${minecraft_version}"],
    "fabricloader": ">=${loader_version}"
  },
  "breaks": {
    "optifabric": "*",
    "feather": "*",
    "origins": "*",
    "sodium": "<0.7.0",
    "morechathistory": "*"
  }
}
